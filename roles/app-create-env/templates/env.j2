# {{ ansible_managed }}

# Environment
COMPOSE_FILE={{ compose_files | join(':') }}
ENV={{ env }}
DJANGO_CONFIGURATION={{ env | capitalize }}
PROJECT_NAME_SUFFIX={{ project_name_suffix | default('') }}
TINI_VERSION={{ lookup('env', 'TINI_VERSION') }}

# PostgreSQL
POSTGRES_USER={{ postgres_user }}
POSTGRES_PASSWORD={{ postgres_password }}
POSTGRES_DB={{ postgres_db }}

# Django
SECRET_KEY={{ django_secret_key }}
